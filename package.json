{
  "name": "ai-trading-agent",
  "version": "1.0.0",
  "description": "AI-powered trading analysis platform with Next.js frontend and FastAPI backend",
  "scripts": {
    "setup": "pnpm install && pnpm approve-builds sqlite3 sharp @nestjs/core @tailwindcss/oxide @clerk/shared unrs-resolver",
    "dev": "./dev.sh",
    "test:setup": "./dev.sh test",
    "start": "./dev.sh start",
    "frontend": "./dev.sh frontend",
    "backend": "./dev.sh backend",
    "stop": "./dev.sh stop",
    "status": "./dev.sh status",
    "clean": "./dev.sh clean",
    "frontend:install": "cd frontend && pnpm install",
    "frontend:build": "cd frontend && pnpm build",
    "frontend:start": "cd frontend && pnpm start",
    "backend:install": "cd backend && pnpm install",
    "backend:build": "cd backend && pnpm build",
    "backend:start": "cd backend && pnpm start",
    "backend:start:dev": "cd backend && pnpm start:dev",
    "backend:test": "cd backend && pnpm test",
    "install:all": "pnpm run backend:install && pnpm run frontend:install",
    "build": "pnpm run backend:build && pnpm run frontend:build",
    "lint": "cd frontend && pnpm lint",
    "test": "cd frontend && pnpm test"
  },
  "keywords": [
    "trading",
    "ai",
    "analysis",
    "nextjs",
    "fastapi",
    "gemini",
    "trading-bot"
  ],
  "author": "AI Trading Agent Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.7.0",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "alphavantage": "^2.5.0",
    "react-hook-form": "^7.63.0",
    "sqlite3": "^5.1.7"
  }
}
