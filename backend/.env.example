# Trader AI Backend (NestJS)

# Application
APP_NAME=Trader AI Backend
APP_VERSION=1.0.0
NODE_ENV=development
PORT=8000

# Database Configuration (SQLite by default)
DB_TYPE=sqlite
DB_PATH=./data/trader-ai.sqlite
# For PostgreSQL instead, comment the lines above and set:
# DB_TYPE=postgres
# DB_HOST=localhost
# DB_PORT=5432
# DB_USERNAME=postgres
# DB_PASSWORD=changeme
# DB_NAME=trader_ai
# or provide a DATABASE_URL=postgresql://user:password@localhost:5432/trader_ai

# Logging
DB_LOGGING=false

# CORS configuration (comma separated)
CORS_ORIGINS=http://localhost:3000

# Market data providers
ALPHA_VANTAGE_API_KEY=demo

# Chat module
CHAT_MEMORY_LIMIT=20

# AI Models Configuration
# Gemini API Key (Google AI - for chat)
GEMINI_API_KEY=your_gemini_api_key_here

# OpenRouter API Key (for Qwen chart analysis)
# Get your API key from: https://openrouter.ai/keys
OPENROUTER_API_KEY=your_openrouter_api_key_here